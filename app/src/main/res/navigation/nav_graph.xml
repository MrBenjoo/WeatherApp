<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/startPageFragment">
    <fragment
        android:id="@+id/startPageFragment"
        android:name="com.benji.weatherswe.searchcity.SearchCityFragment"
        android:label="start_page_fragment"
        tools:layout="@layout/search_city_fragment" >
        <action
            android:id="@+id/action_startPageFragment_to_mainPageFragment"
            app:destination="@id/mainPageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/mainPageFragment"
        android:name="com.benji.weatherswe.weather.WeatherFragment"
        android:label="main_page_fragment"
        tools:layout="@layout/weather_fragment" />
</navigation>